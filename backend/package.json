{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
"scripts": {
  "start": "node src/server.js",
  "dev": "nodemon src/server.js",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/config.js --migrations-path src/migrations",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --config src/config/config.js --migrations-path src/migrations",
    "db:seed": "npx sequelize-cli db:seed:all --config src/config/config.js --seeders-path src/seeders",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all --config src/config/config.js --seeders-path src/seeders",

  "build": "npm install && npm run db:migrate && npm run db:seed"
},


